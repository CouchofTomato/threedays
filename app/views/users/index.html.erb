<% provide(:title, 'All users') %>
<h1>All users</h1>



<div class="flickr_pagination">
<%= will_paginate %>
</div>

	<ul class="users">
		<% @users.each do |user| %>
			<li>
				<h2>
				  <%= gravatar_for user, size: 40 %>
				  <%= user.username %> - 
				  <%= user.email %>
				</h2>
			</li>
		<% end %>
	</ul>

<%= will_paginate %>